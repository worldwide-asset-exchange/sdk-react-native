  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "doctor": "npx expo-doctor",
    "prebuild": "npx expo prebuild",
    "clean": "npx expo prebuild --clean",
    "start": "npx expo start --dev-client",
    "android": "npx expo run:android",
    "android:build": "eas build --platform android --profile android-production",
    "ios": "npx expo run:ios",
    "ios:build": "eas build --platform ios --profile ios-production --auto-submit",
    "web": "npx expo start --web",
    "ld": "yarn && cd ios && pod install && cd .. && yarn run ios --port 8082"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
