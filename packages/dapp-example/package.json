    "expo-router": "~4.0.19",
    "expo-status-bar": "~2.0.1",
    "react": "18.3.1",
    "react-native": "0.76.7",
    "react-native-get-random-values": "~1.8.0",
    "react-native-linear-gradient": "^3.0.0-alpha.1",
    "react-native-qrcode-svg": "^6.3.12",
    "react-native-svg": "15.8.0",
    "react-native-toast-message": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
