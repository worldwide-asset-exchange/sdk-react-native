{
  "name": "@waxio/waxjs-mobile-sdk",
  "version": "1.0.0",
  "description": "Wax Mobile SDK enables developers to easily connect their dapps with the WaxCloudWallet Mobile.",
  "main": "index.js",
  "scripts": {
    "prebuild": "cd packages/sdk-react-native && yarn run prebuild",
    "build": "yarn install && cd packages/sdk-react-native && yarn build",
    "lint": "yarn workspaces foreach --no-private run lint",
    "lint:changelogs": "yarn workspaces foreach --no-private run lint:changelog",
    "lint:eslint": "yarn workspaces foreach --no-private run lint:eslint",
    "lint:fix": "yarn workspaces foreach --no-private run lint:fix",
    "lint:misc": "yarn workspaces foreach --no-private run lint:misc",
    "test": "yarn workspaces foreach --no-private run test"
  },
  "workspaces": [
    "packages/sdk-react-native",
    "packages/example",
    "packages/example-pending"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "conventional-changelog-atom": "^5.0.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-standard": "5.0.0",
    "jest": "^29.6.4",
    "lint-staged": "^13.2.3",
    "patch-package": "^6.4.7",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "serve": "^14.2.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "ts-node": "^10.9.1"
  },
